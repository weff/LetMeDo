<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <GridLayout
      android:layout_centerInParent="true"
        android:background="@color/white"
        android:layout_width="@dimen/length_300"
        android:layout_height="wrap_content"
        android:columnCount="5"
        android:rowCount="7">

        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_237"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>


        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>
        <TextView
            android:gravity="center"
            android:textSize="@dimen/size_12"
            android:textColor="@color/black"
            android:text="@string/nomal"
            android:layout_gravity="center"
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_18"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>

        <RelativeLayout
            android:layout_height="@dimen/length_30"
            android:layout_width="@dimen/length_237"
            android:orientation="horizontal">
        <TextView
            android:id="@+id/normal"
            android:textSize="@dimen/size_12"
            android:gravity="center"
            android:layout_centerInParent="true"
            android:textColor="@color/black"
            android:text="42.5元/3个"
            android:layout_height="@dimen/length_18"
            android:layout_width="@dimen/length_237"/>
        </RelativeLayout>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>

        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_237"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>

        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>
        <TextView
            android:gravity="center"
            android:layout_gravity="center"
            android:textSize="@dimen/size_12"
            android:textColor="@color/black"
            android:text="@string/level"
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_18"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>

        <RelativeLayout
            android:layout_height="@dimen/length_30"
            android:layout_width="@dimen/length_237"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/level"
                android:textSize="@dimen/size_12"
                android:textColor="@color/black"
                android:layout_centerInParent="true"
                android:text="德国"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
        </RelativeLayout>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>

        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_237"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>


        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>
        <TextView
            android:gravity="center"
            android:textSize="@dimen/size_12"
            android:textColor="@color/black"
            android:text="包装"
            android:layout_gravity="center"
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_18"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>

        <RelativeLayout
            android:layout_height="@dimen/length_30"
            android:layout_width="@dimen/length_237"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/pick"
                android:textSize="@dimen/size_12"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:textColor="@color/black"
                android:text="桶"
                android:layout_height="@dimen/length_18"
                android:layout_width="@dimen/length_237"/>
        </RelativeLayout>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_30"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_60"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_237"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>
        <View
            android:layout_width="@dimen/length_1"
            android:layout_height="@dimen/length_1"
            android:background="@color/gray"/>

    </GridLayout>

</RelativeLayout>